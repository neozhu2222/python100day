def prime_checker(number):
  prime = True
  for p in range(2, number):
    if number % p == 0:
      prime = False
  if prime:
    print("its prime")
  else:
    print("not prime")

#Write your code above this line ğŸ‘†
    
#Do NOT change any of the code belowğŸ‘‡
n = int(input("Check this number: "))
prime_checker(number=n)



